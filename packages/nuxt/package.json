{
  "name": "@tailvue/nuxt",
  "version": "0.1.24",
  "description": "Nuxt module for tailvue",
  "private": false,
  "keywords": [
    "tailvue",
    "vue3",
    "vue-3",
    "vue",
    "vue-plugin",
    "tailwindcss",
    "windicss",
    "nuxt-module",
    "nuxt",
    "nuxt3"
  ],
  "homepage": "https://github.com/fumeapp/tailvue#readme",
  "bugs": {
    "url": "https://github.com/fumeapp/tailvue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fumeapp/tailvue.git",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "author": "Kevin Olson <acidjazz@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist/*.cjs",
    "module.cjs",
    "dist/runtime/*.mjs",
    "dist/runtime/*.d.ts"
  ],
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "nuxt3": "latest",
    "tailvue": "latest"
  },
  "peerDependencies": {
    "tailvue": "workspace:*"
  },
  "devDependencies": {
    "unbuild": "^0.5.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
